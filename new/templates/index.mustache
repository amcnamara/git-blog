<!DOCTYPE html>
<html>
  {{> ./partials/_meta_header.mustache}}
  <body>
    {{> ./partials/_content_background.mustache}}
    {{> ./partials/_content_navigation.mustache}}
    <div class="content">
      <h1 class="index">All Posts:</h1>
      {{#runIndex}}
        <div class="post">
          <h5 class="title">{{post.title}}</h6>
          <h6 class="description">{{post.description}}</h6>
          <h6 class="keywords">{{post.keywords}}</h6>
        </div>
      {{/runIndex}}
    </div>
    {{> ./partials/_content_footer.mustache}}
  </body>
</html>
